// Write a function that takes an array of numbers as argument. It should return the sum of the numbers.

function myFunction(a){
let sum = 0;
a.forEach((i) => sum += i)
return sum;
}

myFunction([10,100,40])
Expected
150
myFunction([10,100,1000,1])
Expected
1111
myFunction([-50,0,50,200])
Expected
200
