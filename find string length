//  Write a function that takes two strings (a and b) as arguments. Return the number of times a occurs in b.


function myFunction(a,b){
   
    let result = b.split("").filter( char => char === a);
    return result.length;
}
// in short =>     return b.split(a).length - 1


console.log(myFunction('m','who am i find me'))
